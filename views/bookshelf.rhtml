<h1 style="width:100%;clear:both;">My Digital Bookshelf</h1>
<ul class="book-list">Textbooks
  <li>All of Statistics</li>
  <li>Linear Algebra Done Right</li>
</ul>
<ul class="link-list"><em>Bit.ly</em> Links
  <% collection.each do |title,bundle| %>
    <li><ul><%= title %>
      <% for link in bundle %>
      <li><a href="<%= link.link %>" title="<%= link.long_url %>"><%= link.title %></a></li>
      <% end %>
    </li></ul>
  <% end %>
</ul>
